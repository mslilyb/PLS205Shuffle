library(learnr)
knitr::opts_chunk$set(echo = FALSE)
library(ggplot2)
library(emmeans)
library(multcomp)
library(PLS205)
library(lme4)
library(lmerTest)
potato_data = read.csv('data/Potato_yield.csv',stringsAsFactors = TRUE)
str(potato_data)
potato_data$Plot = factor(potato_data$Plot)
potato_data$Nitrogen = factor(potato_data$Nitrogen)
str(potato_data)
combos = interaction(potato_data$Variety,potato_data$Nitrogen,drop = TRUE)
str(combos)
ggplot(potato_data,aes(x=Nitrogen,y=Yield)) +
geom_boxplot(aes(group = interaction(Nitrogen,Variety),color = Variety))
variety_specific_effects_model <- lm(Yield ~ Nitrogen + Variety:Nitrogen,
data = potato_data)
pls205_diagnostics(variety_specific_effects_model)
